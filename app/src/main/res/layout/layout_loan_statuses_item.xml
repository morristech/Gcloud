<android.support.v7.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginLeft="5dp"
    android:layout_marginRight="5dp"
    android:layout_marginTop="5dp"
    android:clickable="true"
    android:focusable="true"
    android:foreground="?android:attr/selectableItemBackground"
    android:orientation="vertical">

    <LinearLayout
        android:id="@+id/view_stock_list_item"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:layout_gravity="center_vertical"
            android:divider="@color/grey"
            android:gravity="center_vertical"
            android:orientation="horizontal"
            android:padding="10dp">

            <LinearLayout

                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center_vertical"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/tv_label01"
                    style="@style/textStyleHint"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:hint="APPLICATION ID" />

                <TextView
                    android:id="@+id/tv01"
                    style="@style/textStyleHeading2"
                    android:textColor="@color/material_text"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    />
            </LinearLayout>

            <TextView
                android:id="@+id/tv02"
                style="@style/textStyleHint"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true"
                android:layout_marginLeft="@dimen/default_little_margin"
                android:layout_marginRight="@dimen/default_little_margin"
                android:layout_weight="1.2"
                android:background="@drawable/rounded_border_text_view"
                android:gravity="center"
                android:padding="@dimen/default_little_padding"
                android:singleLine="true"
                android:text="Approved"
                android:textColor="@color/material_text"
                android:visibility="invisible"></TextView>

            <ImageView
                android:id="@+id/ivBankLogo"
                android:layout_width="0dp"
                android:layout_height="47dp"
                android:layout_alignParentRight="true"
                android:layout_gravity="center_vertical"
                android:layout_weight="1"

                android:padding="@dimen/default_little_margin">

            </ImageView>


        </LinearLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="1px"

            android:background="@color/grey_light" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:padding="10dp">

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1.4"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/tv_label11"
                    style="@style/textStyleHint"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:hint="NAME" />

                <TextView
                    android:id="@+id/tv11"
                    style="@style/textStyleHeading2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"

                    android:textSize="18sp" />

                <TextView
                    android:id="@+id/tv_label21"
                    style="@style/textStyleHint"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="7dp"
                    android:hint="CAR MODEL" />

                <TextView
                    android:id="@+id/tv21"
                    style="@style/textStyleHeading2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"

                    android:textSize="18sp" />

                <TextView
                    android:id="@+id/tv_label31"
                    style="@style/textStyleHint"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="7dp"
                    android:hint="REGISTRATION NO."
                    android:visibility="gone" />

                <TextView
                    android:id="@+id/tv31"
                    style="@style/textStyleHeading2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"

                    android:textSize="18sp"
                    android:visibility="gone" />


            </LinearLayout>

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/tv_label12"
                    style="@style/textStyleHint"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:hint="CAR MODEL" />

                <TextView
                    android:id="@+id/tv12"
                    style="@style/textStyleHeading2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"

                    android:textSize="18sp" />


                <TextView
                    android:id="@+id/tv_label22"
                    style="@style/textStyleHint"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="7dp"
                    android:hint="NAME" />

                <TextView
                    android:id="@+id/tv22"
                    style="@style/textStyleHeading2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text=""
                    android:textSize="18sp" />

                <TextView
                    android:id="@+id/tv_label32"
                    style="@style/textStyleHint"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="7dp"
                    android:hint="CAR MODEL"
                    android:visibility="gone" />

                <TextView
                    android:id="@+id/tv32"
                    style="@style/textStyleHeading2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"

                    android:textSize="18sp"
                    android:visibility="gone" />

            </LinearLayout>
        </LinearLayout>

        <TextView
            android:id="@+id/tv_label41"
            style="@style/textStyleHint"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="10dp"
            android:hint="REASON "
            android:visibility="gone" />

        <TextView
            android:id="@+id/tv41"
            style="@style/textStyleHeading2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"

            android:textSize="18sp"
            android:visibility="gone" />
    </LinearLayout>

</android.support.v7.widget.CardView><!--
</LinearLayout>-->
