                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"

                                android:layout_centerVertical="true"
                                android:text="Color" />

                            <TextView
                                android:id="@+id/stockcolorvalue"
                                style="@style/textStyleHeading2.customFont"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_centerVertical="true"
                                android:text="White"
