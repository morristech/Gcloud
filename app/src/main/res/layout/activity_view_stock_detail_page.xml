                        android:layout_height="56dp"
                        android:layout_below="@+id/makeModelColorLayout"
                        android:baselineAligned="false"
                        android:orientation="horizontal"
                        android:paddingBottom="4dp"
                        android:paddingTop="4dp">

                        <LinearLayout
