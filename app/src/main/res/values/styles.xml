<resources xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <item name="android:textViewStyle">@style/TextViewCustomFont</item>
        <!-- Customize your theme here. -->
    </style>
    <style name="FinanceImageUploadProgressBar">
        <item name="android:layout_width">40dp</item>
        <item name="android:layout_height">40dp</item>
        <item name="android:layout_centerInParent">true</item>
        <item name="android:indeterminate">true</item>
        <item name="android:visibility">gone</item>
    </style>
    <style name="full_screen_dialog" parent="AlertDialog.AppCompat.Light">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowBackground">@color/gray_light2</item>
        <item name="android:windowAnimationStyle">@android:style/Animation.Dialog</item>
        <item name="android:windowSoftInputMode">stateUnspecified|adjustPan</item>

    </style>
    <!-- Progress Dialog full screen -->
    <style name="progressDialog" parent="AlertDialog.AppCompat.Light">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:textViewStyle">@style/TextViewCustomFont</item>
    </style>
    <!-- date time picker -->
    <style name="time_label">
        <item name="android:textSize">@dimen/time_label_size</item>
        <item name="android:textColor">@color/numbers_text_color</item>
        <item name="android:textViewStyle">@style/TextViewCustomFont</item>
    </style>
    <style name="ampm_label">
        <item name="android:textSize">@dimen/ampm_label_size</item>
        <item name="android:textColor">@color/ampm_text_color</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textViewStyle">@style/TextViewCustomFont</item>
    </style>
    <style name="day_of_week_label_condensed" />
    <style name="textStyleHeadingCenter">
        <item name="android:color">@color/textColor</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:gravity">center</item>
        <item name="android:textAppearance">@style/TextAppearance.FontPath</item>
    </style>
    <style name="textStyleHeading">
        <item name="android:color">@color/textColor</item>
        <item name="android:textColor">@color/textColor</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textStyle">normal</item>
        <item name="android:singleLine">true</item>
        <item name="android:ellipsize">end</item>
        <!--<item name="android:textAppearance">@style/TextAppearance.FontPath</item>-->
    </style>
    <style name="gc_button">
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">16sp</item>
        <item name="android:background">@color/primary</item>
        <item name="android:singleLine">true</item>
        <item name="android:ellipsize">end</item>
        <!--<item name="android:textAppearance">@style/TextAppearance.FontPath</item>-->
    </style>
    <style name="gc_dark_textview">
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">16sp</item>
        <!--<item name="android:background">@color/gc_button_background</item>-->
        <item name="android:singleLine">true</item>
        <item name="android:ellipsize">end</item>
        <!--<item name="android:textAppearance">@style/TextAppearance.FontPath</item>-->
    </style>
    <style name="gc_textview_nobackground">
        <item name="android:textColor">@color/textColor</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:singleLine">true</item>
        <item name="android:ellipsize">end</item>
        <item name="android:textAppearance">@style/TextAppearance.FontPath</item>
    </style>
    <style name="textStyleWhite">
        <item name="android:color">@color/white</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textAppearance">@style/TextAppearance.FontPath</item>
    </style>

    <style name="TextAppearance.FontPath" parent="android:TextAppearance">
        <!-- Custom Attr-->
        <item name="fontPath">fonts/nunito_regular.ttf</item>
    </style>

    <style name="TextViewCustomFont">
        <item name="fontPath">fonts/nunito_regular.ttf</item>
    </style>

    <style name="radio_button_style">
        <item name="android:button">@null</item>
        <item name="android:background">@drawable/radio_button_background</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">@color/textColor</item>
        <item name="android:gravity">center_vertical|left</item>
    </style>
    <style name="radio_button_style_wd_orange_bg">
        <item name="android:button">@null</item>
        <item name="android:background">@drawable/radio_btn_wd_orange_bg</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">@color/textColor</item>
        <item name="android:gravity">center_vertical|left</item>
    </style>
    <style name="textStyleHeading2">
        <item name="android:color">@color/textColor</item>
        <item name="android:textColor">@color/textColor</item>
        <item name="android:textColorHint">@color/hintColor</item>
        <item name="android:singleLine">true</item>
        <item name="android:ellipsize">end</item>
        <item name="android:textSize">14sp</item>
        <item name="android:imeOptions">actionDone</item>
        <item name="android:textAppearance">@style/TextAppearance.FontPath</item>
    </style>
    <style name="textStyleHint">
        <item name="android:color">@color/hintColor</item>
        <item name="android:textColor">@color/hintColor</item>
        <item name="android:textColorHint">@color/hintColor</item>
        <item name="android:singleLine">true</item>
        <item name="android:ellipsize">end</item>
        <item name="android:textSize">10sp</item>
        <item name="android:imeOptions">actionDone</item>
        <item name="android:textAppearance">@style/TextAppearance.FontPath</item>
    </style>
    <style name="textStyleHeading2Oragne">
        <item name="android:color">@color/textColorOrage</item>
        <item name="android:textColor">@color/textColorOrage</item>
        <item name="android:textColorHint">@color/hintColor</item>
        <item name="android:singleLine">true</item>
        <item name="android:ellipsize">end</item>
        <item name="android:textSize">14sp</item>
        <item name="android:imeOptions">actionDone</item>
        <item name="android:textAppearance">@style/TextAppearance.FontPath</item>
    </style>
    <style name="textStyleHeading2.customFont" parent="@style/textStyleHeading2">
        <item name="android:textSize">12sp</item>
        <item name="android:textColor">@color/hintColor</item>
        <item name="android:textViewStyle">@style/TextViewCustomFont</item>
    </style>
    <style name="textStyleHeading2.customFont2" parent="@style/textStyleHeading2">
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">@color/hintColor</item>
        <item name="android:textViewStyle">@style/TextViewCustomFont</item>
    </style>
    <style name="textSpinnerStyleHeading">
        <item name="android:color">@color/textColor</item>
        <item name="android:textColor">@color/textColor</item>
        <item name="android:textColorHint">@color/hintColor</item>
        <item name="android:textColorHighlight">@color/hintColor</item>
        <item name="android:singleLine">true</item>
        <item name="android:ellipsize">end</item>
        <item name="android:textSize">14sp</item>
        <item name="android:paddingLeft">8dp</item>
        <item name="android:gravity">center_vertical|left</item>
        <item name="android:background">@drawable/custom_dropdown</item>
        <item name="android:imeOptions">actionDone</item>
        <item name="android:textAppearance">@style/TextAppearance.FontPath</item>
    </style>
    <style name="editTextStyle">
        <item name="android:color">@color/textColor</item>
        <item name="android:textColor">@color/textColor</item>
        <item name="android:textColorHint">@color/hintColor</item>
        <item name="android:textColorHighlight">@color/hintColor</item>
        <item name="android:background">@drawable/abc_edit_text_material</item>
        <!--<item name="android:singleLine">true</item>-->
        <item name="android:textSize">14sp</item>
        <item name="android:paddingLeft">8dp</item>
        <item name="android:textCursorDrawable">@null</item>
        <item name="android:gravity">left|center_vertical</item>
        <item name="android:imeOptions">actionDone</item>
        <item name="android:textAppearance">@style/TextAppearance.FontPath</item>
        <!--<item name="android:ellipsize">end</item>-->
    </style>
    <style name="editTextStyle2">
        <item name="android:color">@color/textColor</item>
        <item name="android:textColor">@color/textColor</item>
        <item name="android:textColorHint">@color/hintColor</item>
        <item name="android:textColorHighlight">@color/hintColor</item>
        <!--<item name="android:singleLine">true</<item>-->
        <item name="android:textSize">14sp</item>
        <item name="android:paddingLeft">8dp</item>
        <item name="android:textCursorDrawable">@null</item>
        <item name="android:gravity">left|center_vertical</item>
        <item name="android:imeOptions">actionDone</item>
        <item name="android:textAppearance">@style/TextAppearance.FontPath</item>
        <!--<item name="android:ellipsize">end</item>-->
    </style>
    <style name="textInputLayoutStyle" parent="TextAppearance.AppCompat">
        <item name="android:color">@color/lms_orange</item>
        <item name="android:textColor">@color/lms_orange</item>
        <item name="android:textColorHint">@color/gray</item>
        <item name="android:textSize">18sp</item>
        <item name="android:paddingLeft">0dp</item>
        <item name="android:textCursorDrawable">@null</item>
        <item name="android:gravity">left|center_vertical</item>
        <item name="android:imeOptions">actionDone</item>
        <item name="android:textAppearance">@style/TextAppearance.FontPath</item>
        <item name="colorAccent">@color/gray</item>
        <item name="colorControlNormal">@color/gray</item>
        <item name="colorControlHighlight">@color/lms_orange</item>
        <item name="colorControlActivated">@color/lms_orange</item>
        <!--<item name="android:ellipsize">end</item>-->
    </style>

    <style name="autoCompleteTextViewStyle">
        <item name="android:textColor">@drawable/autocomplete_textcolor</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textColorHint">@color/hintColor</item>
        <item name="android:textColorHighlight">@color/hintColor</item>
        <item name="android:background">@drawable/abc_edit_text_material</item>

        <item name="android:imeOptions">actionDone</item>
        <item name="android:textAppearance">@style/TextAppearance.FontPath</item>

    </style>

    <style name="gc_light_textview">
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">16sp</item>
        <item name="android:background">@color/background_material_light</item>
        <item name="android:singleLine">true</item>
        <item name="android:ellipsize">end</item>
        <item name="android:textAppearance">@style/TextAppearance.FontPath</item>
    </style>
    <style name="fab_counter_light_textview">
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">12sp</item>
        <item name="android:background">@drawable/counter_view</item>
        <item name="android:singleLine">true</item>
        <item name="android:ellipsize">end</item>

        <item name="android:textAppearance">@style/TextAppearance.FontPath</item>
    </style>

    <style name="text_bold">
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/textColor</item>
    </style>

    <style name="text_normal">
        <item name="android:textStyle">normal</item>
        <item name="android:textColor">@color/textColor</item>
    </style>

    <!-- LMS Start -->
    <style name="LMSButton">
        <item name="android:background">@android:color/transparent</item>
        <item name="android:padding">20dp</item>
        <item name="android:gravity">center</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">12sp</item>
        <item name="android:layout_height">52dp</item>
        <item name="android:textColor">@color/textColor</item>
    </style>

    <style name="AnimationDialog">
        <item name="android:windowEnterAnimation">@android:anim/slide_in_left</item>
        <item name="android:windowExitAnimation">@android:anim/slide_out_right</item>
    </style>

    <style name="LMSDialogActivityTheme" parent="Theme.AppCompat.Dialog">
        <item name="android:background">@drawable/lms_activity_bg</item>
        <item name="background">@drawable/lms_activity_bg</item>
        <!--<item name="android:datePickerDialogTheme">@style/MyDatePickerStyle</item>-->
    </style>

    <style name="LMSDialogTheme" parent="Theme.AppCompat.Dialog.Alert">
        <item name="android:textColor">@color/lms_dark_gray</item>
        <item name="android:textStyle">bold</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="finance_form_theme" parent="AppCompatNoActionBarTheme">
        <item name="colorControlNormal">@color/grey</item>
        <item name="colorAccent">@color/orange</item>
    </style>

    <style name="menu_labels_style">
        <item name="android:background">@drawable/fab_label_background</item>
        <item name="android:textColor">@color/white</item>
    </style>
    <style name="AppCompatNoActionBarTheme.AppBarOverlay"
        parent="ThemeOverlay.AppCompat.Dark.ActionBar" />
    <style name="AppCompatNoActionBarTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

    <style name="CustomRatingBar" parent="@android:style/Widget.Holo.RatingBar.Small">
    <item name="android:progressDrawable">@drawable/custom_rating_bar</item>
                <item name="android:minHeight">48dip</item>
                <item name="android:maxHeight">96dip</item>
</style>
    <!-- LMS End -->
</resources>
